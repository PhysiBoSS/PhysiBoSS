<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>-500</x_min>
		<x_max>500</x_max>
		<y_min>-500</y_min>
		<y_max>500</y_max>
		<z_min>-500</z_min>
		<z_max>500</z_max>
		<dx>15</dx>
		<dy>15</dy>
		<dz>15</dz>
		<use_2D>false</use_2D>
	</domain>

	<overall>
		<max_time units="min">10080</max_time>
		<time_units>min</time_units>
		<space_units>micron</space_units>
        <dt_diffusion units="min">0.01</dt_diffusion>
        <dt_mechanics units="min">0.1</dt_mechanics>
        <dt_phenotype units="min">6</dt_phenotype>
	</overall>

	<parallel>
		<omp_num_threads>4</omp_num_threads>
	</parallel>

	<save>
		<folder>output_lumi</folder>
		<full_data>
			<interval units="min">240</interval>
			<enable>true</enable>
		</full_data>
		<SVG>
			<interval units="min">240</interval>
			<enable>true</enable>
			<legend enabled="true">
				<cell_phase>false</cell_phase>
				<cell_type>true</cell_type>
			</legend>
		</SVG>
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>

	<options>
		<legacy_random_points_on_sphere_in_divide>false</legacy_random_points_on_sphere_in_divide>
		<virtual_wall_at_domain_edge>true</virtual_wall_at_domain_edge>
		<disable_automated_spring_adhesions>false</disable_automated_spring_adhesions>
	</options>

	<microenvironment_setup>
		<variable name="oxygen" units="mmHg" ID="0">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">100000.0</diffusion_coefficient>
				<decay_rate units="1/min">0.0005</decay_rate>
			</physical_parameter_set>
			<initial_condition units="mmHg">160.0</initial_condition>
			<Dirichlet_boundary_condition units="mmHg" enabled="true">160.0</Dirichlet_boundary_condition>
			<Dirichlet_options>
				<boundary_value ID="xmin" enabled="True">38.0</boundary_value>
				<boundary_value ID="xmax" enabled="True">38.0</boundary_value>
				<boundary_value ID="ymin" enabled="True">38.0</boundary_value>
				<boundary_value ID="ymax" enabled="True">38.0</boundary_value>
				<boundary_value ID="zmin" enabled="True">38.0</boundary_value>
				<boundary_value ID="zmax" enabled="True">38.0</boundary_value>
			</Dirichlet_options>
	</variable>
	<variable name="Pictilisib" units="mmol" ID="1">
		<physical_parameter_set>
			<diffusion_coefficient units="micron^2/min">1200.0</diffusion_coefficient>
			<decay_rate units="1/min">0.0275</decay_rate>
		</physical_parameter_set>
		<initial_condition units="mmHg">0.0</initial_condition>
		<Dirichlet_boundary_condition units="mmHg" enabled="False">0.0</Dirichlet_boundary_condition>
		<Dirichlet_options>
			<boundary_value ID="xmin" enabled="False">0.0</boundary_value>
			<boundary_value ID="xmax" enabled="False">0.0</boundary_value>
			<boundary_value ID="ymin" enabled="False">0.0</boundary_value>
			<boundary_value ID="ymax" enabled="False">0.0</boundary_value>
			<boundary_value ID="zmin" enabled="False">0.0</boundary_value>
			<boundary_value ID="zmax" enabled="False">0.0</boundary_value>
		</Dirichlet_options>
	</variable>
	<variable name="Luminespib" units="mmol" ID="2">
		<physical_parameter_set>
			<diffusion_coefficient units="micron^2/min">1200.0</diffusion_coefficient>
			<decay_rate units="1/min">0.0275</decay_rate>
		</physical_parameter_set>
			<initial_condition units="mmHg">160.0</initial_condition>
			<Dirichlet_boundary_condition units="mmHg" enabled="true">160.0</Dirichlet_boundary_condition>
			<Dirichlet_options>
				<boundary_value ID="xmin" enabled="True">38.0</boundary_value>
				<boundary_value ID="xmax" enabled="True">38.0</boundary_value>
				<boundary_value ID="ymin" enabled="True">38.0</boundary_value>
				<boundary_value ID="ymax" enabled="True">38.0</boundary_value>
				<boundary_value ID="zmin" enabled="True">38.0</boundary_value>
				<boundary_value ID="zmax" enabled="True">38.0</boundary_value>
			</Dirichlet_options>
	</variable>
	<options>
		<calculate_gradients>true</calculate_gradients>
		<track_internalized_substrates_in_each_agent>true</track_internalized_substrates_in_each_agent>
		<!-- not yet supported -->
		<initial_condition type="matlab" enabled="false">
			<filename>./config/initial.mat</filename>
		</initial_condition>
		<!-- not yet supported -->
		<dirichlet_nodes type="matlab" enabled="false">
			<filename>./config/dirichlet.mat</filename>
		</dirichlet_nodes>
	</options>
</microenvironment_setup>

<cell_definitions>
	<cell_definition name="default" ID="0">
		<phenotype>
			<cycle code="5" name="live_cells_cycle_model">
				<phase_transition_rates units="1/min">
					<rate start_index="0" end_index="0" fixed_duration="false">0.0003155</rate>
				</phase_transition_rates>
			</cycle>

			<death>
				<model code="100" name="apoptosis">
					<death_rate units="1/min">5.31667e-05</death_rate>
					<transition_rates units="1/min">
						<rate start_index="0" end_index="1" fixed_duration="true">0.00193798</rate>
					</transition_rates>
					<parameters>
						<unlysed_fluid_change_rate units="1/min">0.05</unlysed_fluid_change_rate>
						<lysed_fluid_change_rate units="1/min">0</lysed_fluid_change_rate>
						<cytoplasmic_biomass_change_rate units="1/min">1.66667e-02</cytoplasmic_biomass_change_rate>
						<nuclear_biomass_change_rate units="1/min">5.83333e-03</nuclear_biomass_change_rate>
						<calcification_rate units="1/min">0</calcification_rate>
						<relative_rupture_volume units="dimensionless">2.0</relative_rupture_volume>
					</parameters>
				</model>

				<model code="101" name="necrosis">
					<death_rate units="1/min">0.0</death_rate>
					<transition_rates units="1/min">
						<rate start_index="0" end_index="1" fixed_duration="false">9e9</rate>
						<rate start_index="1" end_index="2" fixed_duration="true">1.15741e-5</rate>
					</transition_rates>
					<parameters>
						<unlysed_fluid_change_rate units="1/min">0.05</unlysed_fluid_change_rate>
						<lysed_fluid_change_rate units="1/min">0</lysed_fluid_change_rate>
						<cytoplasmic_biomass_change_rate units="1/min">1.66667e-02</cytoplasmic_biomass_change_rate>
						<nuclear_biomass_change_rate units="1/min">5.83333e-03</nuclear_biomass_change_rate>
						<calcification_rate units="1/min">0</calcification_rate>
						<relative_rupture_volume units="dimensionless">2.0</relative_rupture_volume>
					</parameters>
				</model>
			</death>
			<volume>
				<total units="micron^3">2494</total>
				<fluid_fraction units="dimensionless">0.75</fluid_fraction>
				<nuclear units="micron^3">540</nuclear>
				<fluid_change_rate units="1/min">0.05</fluid_change_rate>
				<cytoplasmic_biomass_change_rate units="1/min">0.0045</cytoplasmic_biomass_change_rate>
				<nuclear_biomass_change_rate units="1/min">0.0055</nuclear_biomass_change_rate>
				<calcified_fraction units="dimensionless">0</calcified_fraction>
				<calcification_rate units="1/min">0</calcification_rate>
				<relative_rupture_volume units="dimensionless">2.0</relative_rupture_volume>
			</volume>
			<mechanics>
				<cell_cell_adhesion_strength units="micron/min">0.4</cell_cell_adhesion_strength>
				<cell_cell_repulsion_strength units="micron/min">100.0</cell_cell_repulsion_strength>
				<relative_maximum_adhesion_distance units="dimensionless">1.1</relative_maximum_adhesion_distance>
				<cell_adhesion_affinities>
					<cell_adhesion_affinity name="epithelial">1.0</cell_adhesion_affinity>
					<cell_adhesion_affinity name="mesenchymal">0.8</cell_adhesion_affinity>
				</cell_adhesion_affinities>
				<options>
					<set_relative_equilibrium_distance enabled="false" units="dimensionless">1.8</set_relative_equilibrium_distance>
					<set_absolute_equilibrium_distance enabled="false" units="micron">15.12</set_absolute_equilibrium_distance>
				</options>
				<cell_BM_adhesion_strength units="micron/min">4.0</cell_BM_adhesion_strength>
				<cell_BM_repulsion_strength units="micron/min">10.0</cell_BM_repulsion_strength>
				<attachment_elastic_constant units="1/min">0.1</attachment_elastic_constant>
				<attachment_rate units="1/min">1.0</attachment_rate>
				<detachment_rate units="1/min">0.0</detachment_rate>
			</mechanics>
			<motility>
				<speed units="micron/min">1</speed>
				<persistence_time units="min">1</persistence_time>
				<migration_bias units="dimensionless">0</migration_bias>

				<options>
					<enabled>false</enabled>
					<use_2D>false</use_2D>
					<chemotaxis>
						<enabled>false</enabled>
						<substrate>oxygen</substrate>
						<direction>1</direction>
					</chemotaxis>
					<advanced_chemotaxis>
						<enabled>false</enabled>
						<normalize_each_gradient>false</normalize_each_gradient>
						<chemotactic_sensitivities>
							<chemotactic_sensitivity substrate="oxygen">0.0</chemotactic_sensitivity>
							<chemotactic_sensitivity substrate="ecm">0.0</chemotactic_sensitivity>
							<chemotactic_sensitivity substrate="tgfbeta">0.0</chemotactic_sensitivity>
							<chemotactic_sensitivity substrate="light">0.0</chemotactic_sensitivity>
						</chemotactic_sensitivities>
					</advanced_chemotaxis>
				</options>
			</motility>
			<secretion>
				<substrate name="oxygen">
					<secretion_rate units="1/min">0</secretion_rate>
					<secretion_target units="substrate density">38</secretion_target>
					<uptake_rate units="1/min">10</uptake_rate>
					<net_export_rate units="total substrate/min">0</net_export_rate>
				</substrate>
				<substrate name="Pictilisib">
					<secretion_rate units="1/min">0.0</secretion_rate>
					<secretion_target units="substrate density">1.0</secretion_target>
					<uptake_rate units="1/min">0.0025</uptake_rate>
					<net_export_rate units="total substrate/min">0.0</net_export_rate>
				</substrate>
				<substrate name="Luminespib">
					<secretion_rate units="1/min">0.0</secretion_rate>
					<secretion_target units="substrate density">1.0</secretion_target>
					<uptake_rate units="1/min">0.0025</uptake_rate>
					<net_export_rate units="total substrate/min">0.0</net_export_rate>
				</substrate>

			</secretion>
			<cell_interactions>
				<dead_phagocytosis_rate units="1/min">0.0</dead_phagocytosis_rate>
				<live_phagocytosis_rates>
					<phagocytosis_rate name="epithelial" units="1/min">0.0</phagocytosis_rate>
					<phagocytosis_rate name="mesenchymal" units="1/min">0.0</phagocytosis_rate>
				</live_phagocytosis_rates>
				<attack_rates>
					<attack_rate name="epithelial" units="1/min">0.0</attack_rate>
					<attack_rate name="mesenchymal" units="1/min">0.0</attack_rate>
				</attack_rates>
				<damage_rate units="1/min">1.0</damage_rate>
				<fusion_rates>
					<fusion_rate name="epithelial" units="1/min">0.0</fusion_rate>
					<fusion_rate name="mesenchymal" units="1/min">0.0</fusion_rate>
				</fusion_rates>
			</cell_interactions>
			<cell_transformations>
				<transformation_rates>
				</transformation_rates>
			</cell_transformations>
			<intracellular type="maboss">
				<bnd_filename>./config/boolean_network/LNCaP_mut_RNA_00_all_nodes.bnd</bnd_filename>
				<cfg_filename>./config/boolean_network/LNCaP_mut_RNA_00_all_nodes.cfg</cfg_filename>
				<settings>
					<intracellular_dt>6.0</intracellular_dt>
					<time_stochasticity>0.0</time_stochasticity>
					<scaling>10.0</scaling>
					<start_time>0.0</start_time>
					<inheritance global="True" />
					<mutations>
					</mutations>
				</settings>
				<mapping>
				</mapping>
			</intracellular>
		</phenotype>
		<custom_data>
			<motility_state>0</motility_state>
			<Pictilisib_concentration units="dimensionless">0.0</Pictilisib_concentration>
			<Pictilisib_node units="dimensionless">0.0</Pictilisib_node>
			<Luminespib_concentration units="dimensionless">0.0</Luminespib_concentration>
			<Luminespib_node units="dimensionless">0.0</Luminespib_node>
		</custom_data>
	</cell_definition>
	<!-- TODO: resistant and sensitive cell_def -->
	<!-- <cell_definition name="Pictilisib_sensitive" ID="1" parent_type="default"/>
		<cell_definition name="Pictilisib_resistant" ID="2" parent_type="default"/>
		<cell_definition name="Luminespib_sensitive" ID="3" parent_type="default"/>
		<cell_definition name="Luminespib_resistant" ID="4" parent_type="default"/> -->
</cell_definitions>

<initial_conditions>
	<cell_positions enabled="true" type="csv">
		<folder>./config</folder>
		<filename>init2.csv</filename>
	</cell_positions>
</initial_conditions>

<user_parameters>
	<random_seed type="int" units="dimensionless">0</random_seed>
	<!-- motility parameters -->
	<migration_bias type="double" units="" description="change value of migration bias for cells with migration node active">0</migration_bias>
	<migration_speed type="double" units="" description="change value of migration speed for cells with migration node active">0.35</migration_speed>
	<persistence type="double" units="" description="change value of persistence for cells with migration node active">2</persistence>

	<!-- proliferation switch implementation -->
	<transition_rate_multiplier type="double" units="">2</transition_rate_multiplier>
	<base_transition_rate type="double" units="">0.0003155</base_transition_rate>

	<!-- Apoptosis rate implementation -->
	<apoptosis_rate_multiplier type="double" units="">5</apoptosis_rate_multiplier>
	<cell_line type="string" units="">LNCaP</cell_line>
	<!-- <bnd_file type="string" units="">./config/boolean_network/LNCaP_mut_RNA_00.bnd</bnd_file>
		<cfg_file type="string" units="">./config/boolean_network/LNCaP_mut_RNA_00.cfg</cfg_file> -->
	<!-- <maboss_time_step type="double" units="dimensionless">10.</maboss_time_step> -->
	<!-- <init_cells_filename type="string" units="">./config/init.txt</init_cells_filename> -->
	<simulation_mode type="int">1</simulation_mode>
	<!-- <membrane_length type="int" units=""> 470 </membrane_length> -->
	<Pictilisib_uptake_rate type="double" units="">0.0025</Pictilisib_uptake_rate>
	<Pictilisib_secretion_rate type="double" units="fg/cell/min">0.1</Pictilisib_secretion_rate>
	<Pictilisib_scal units="dimensionless" type="double">2.30653618101248</Pictilisib_scal>
	<Pictilisib_xmid units="ln(uM)" type="double">3.85680179514967</Pictilisib_xmid>
	<Pictilisib_maxc units="uM" type="double">10.0</Pictilisib_maxc>
	<prop_drug_resistant_Pictilisib type="double" units="dimensionless">0.0</prop_drug_resistant_Pictilisib>
	<drug_concentration_Pictilisib type="string" units="dimensionless">0</drug_concentration_Pictilisib> <!-- IC90 -->
	<Luminespib_uptake_rate type="double" units="">0.0025</Luminespib_uptake_rate>
	<Luminespib_secretion_rate type="double" units="fg/cell/min">0.1</Luminespib_secretion_rate>
	<Luminespib_scal units="dimensionless" type="double">2.30653618101248</Luminespib_scal>
	<Luminespib_xmid units="ln(uM)" type="double">7.02401783651853</Luminespib_xmid>
	<Luminespib_maxc units="uM" type="double">0.25</Luminespib_maxc>
	<prop_drug_resistant_Luminespib type="double" units="dimensionless">0.0</prop_drug_resistant_Luminespib>
	<drug_concentration_Luminespib type="string" units="dimensionless">IC90</drug_concentration_Luminespib> <!-- IC90 -->
</user_parameters>
</PhysiCell_settings>
